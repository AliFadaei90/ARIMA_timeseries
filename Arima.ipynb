plt.xlim(datetime.datetime(L1.year,L1.month,L1.day-1),datetime.datetime(FL1.year,FL1.month,FL1.day))
plt.ylim(ymin=0.999*mC33.values.min(),ymax=1.001*mC33.values.max())
plt.grid(True)
plt.legend(loc='upper left')
plt.show()
